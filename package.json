{
  "name": "procuriosity-public-portal-front",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://public-portal.vtshark.esy.es",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,vue}' 'src/*.{js,vue}'",
    "fix-style": "prettier-stylelint --write 'src/**/*.{css,scss,vue}' 'src/*.{css,scss,vue}' "
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.8.6",
    "element-ui": "^2.5.4",
    "idx": "^2.5.3",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.6",
    "vue-i18n": "^8.8.2",
    "vue-router": "^3.0.2",
    "vue2-scrollspy": "^2.3.1",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.0.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-prettier": "^1.0.6",
    "vue-template-compiler": "^2.6.6"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
